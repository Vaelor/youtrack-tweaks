{
  "name": "YouTrack Tweaks",
  "description": "Add more functionality to most powerful issue tracker in the world. Customize Agile Board cards, hotkeys and automate everyday process. All these features are here, in YouTrack Tweaks Chrome Extension.",
  "version": "0.0.0.1",
  "manifest_version": 2,
  "icons": {
    "128": "images/128.png",
    "256": "images/256.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [ "webNavigation", "*://*/*", "storage", "tabs"],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://extension.youtrack-tweaks.com http://localhost:4200; object-src 'self'",
  "content_scripts": [{
    "js": ["content_script.js"],
    "matches": ["*://*/*"]
  }]
}