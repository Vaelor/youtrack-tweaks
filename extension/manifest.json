{
  "name": "YouTrack Tweaks",
  "description": "Add more functionality to YouTrack. Customize your agile board, hotkeys, and much more. All these features here, in YouTrack Tweaks.",
  "version": "0.0.0.8",
  "manifest_version": 2,
  "icons": {
    "128": "images/128.png",
    "256": "images/256.png"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_icon": {
      "16": "images/16.png",
      "32": "images/32.png"
    },
    "default_popup": "popup.html"
  },
  "permissions": [ "webNavigation", "*://*/*", "storage", "tabs"],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://extension.youtrack-tweaks.com http://localhost:4200; object-src 'self'",
  "content_scripts": [{
    "js": ["content_script.js"],
    "matches": ["*://*/*"]
  }],
  "web_accessible_resources": [
    "images/*.png",
    "repository/*"
  ]
}